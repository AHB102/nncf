{
    "model": "googlenet",
    "batch_size" : 512,
    "pretrained": true,
    "epochs": 100,
    "to_onnx": "/home/mkaglins/work/googlenet_pruned.onnx",
    "input_info": {
        "sample_size": [
            1,
            3,
            224,
            224
        ]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            20,
            40,
            60,
            80
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
       "compression": [
       {
        "algorithm": "filter_pruning",
        "params": {
            "schedule": "baseline",
            "prune_first_conv": true,
            "prune_last_conv": true,
            "prune_downsample_convs" : true,
            "pruning_init": 0.0,
            "pruning_target": 0.3,
            "weight_importance": "geometric_median"
        }
        }
       ]
}
